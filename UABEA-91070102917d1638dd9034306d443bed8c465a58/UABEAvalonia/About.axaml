        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        mc:Ignorable="d" d:DesignWidth="400" d:DesignHeight="210"
        Width="400" Height="210"
        Icon="/Assets/uabeavalonia16.ico"
